{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "NODE_ENV=production node scripts/copy-content.js && NODE_ENV=production node scripts/prepare-next.js && NODE_ENV=production next build",
    "start": "next start",
    "start:next": "if [ -f .next/standalone/server.js ]; then node .next/standalone/server.js; else next start; fi",
    "lint": "next lint",
    "prewarm:ssg": "node scripts/prewarm-ssg.js",
    "typecheck": "tsc -p ./tsconfig.json --noEmit",
    "test:sitemap": "node test-sitemap.js",
    "test:env-vars": "node test-env-vars.js"
  },
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "@heroicons/react": "^2.2.0",
    "@sentry/nextjs": "^10.21.0",
    "bcryptjs": "^3.0.2",
    "critters": "^0.0.23",
    "graphql": "^16.11.0",
    "next": "15.3.5",
    "next-auth": "^4.24.11",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sass": "^1.93.2",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "playwright": "^1.38.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "eslintIgnore": [
    "**/*.bak.ts",
    "**/*.bak.tsx",
    "**/*.bak.js",
    "**/*.bak.jsx",
    "src/middleware.ts.disabled"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}