      {
        "id": 6,
        "type": "stat",
        "title": "API Availability SLO",
        "gridPos": {
          "x": 12,
          "y": 16,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "(1 - sum(increase(http_requests_total{code=~\"5..\"}[30d])) / sum(increase(http_requests_total[30d]))) * 100",
            "refId": "A"
          }
        ],
        "datasource": "Prometheus",
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "decimals": 2,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
