{
  "dashboard": {
    "id": null,
    "title": "GlassCode Academy Observability Dashboard",
    "tags": ["glasscode", "observability"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "5s",
    "panels": [
      {
        "id": 1,
        "type": "graph",
        "title": "Request Rate",
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum(rate(http_requests_total[5m])) by (job)",
            "legendFormat": "{{job}}",
            "refId": "A"
          }
        ],
        "datasource": "Prometheus",
        "yaxes": [
          {
            "format": "reqps",
            "label": "Requests per second"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 2,
        "type": "graph",
        "title": "Error Rate",
        "gridPos": {
          "x": 12,
          "y": 0,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{code=~\"5..\"}[5m])) by (job) / sum(rate(http_requests_total[5m])) by (job)",
            "legendFormat": "{{job}}",
            "refId": "A"
          }
        ],
        "datasource": "Prometheus",
        "yaxes": [
          {
            "format": "percentunit",
            "label": "Error rate"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 3,
        "type": "graph",
        "title": "Latency (p95)",
        "gridPos": {
          "x": 0,
          "y": 8,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le, job))",
            "legendFormat": "{{job}}",
            "refId": "A"
          }
        ],
        "datasource": "Prometheus",
        "yaxes": [
          {
            "format": "s",
            "label": "Latency"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 4,
        "type": "graph",
        "title": "Database Query Performance",
        "gridPos": {
          "x": 12,
          "y": 8,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(db_query_duration_seconds_bucket[5m])) by (le, job))",
            "legendFormat": "{{job}}",
            "refId": "A"
          }
        ],
        "datasource": "Prometheus",
        "yaxes": [
          {
            "format": "s",
            "label": "Query duration"
          },
          {
            "format": "short"
          }
        ]
      }
    ],
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    },
    "links": []
  }
}